<module>

    <!-- Inherit the core Web Toolkit stuff.                  -->
    <inherits name='com.google.gwt.user.User'/>

    <!-- Specify the app entry point class.                   -->
    <entry-point class='filedownloadfacility.client.FileDownloadFacility'/>


    <!-- Specify the app servlets.                   -->
    <servlet path='/filedownloadfacility.FileDownloadFacility/FileDownloadFacilityService'
             class='filedownloadfacility.server.FileDownloadFacilityServiceImpl'/>

    <servlet path='/FileDownloadFacilityServlet' class='filedownloadfacility.server.FileDownloadFacilityServlet'/> 

</module>
<module>

    <!-- Inherit the core Web Toolkit stuff. -->
    <inherits name='com.google.gwt.user.User'/>

    <!-- Include GWTUpload library -->
    <inherits name="gwtupload.GWTUpload"/>
    <inherits name="com.google.gwt.widgetideas.HyperlinkOverride"/>
    <inherits name="gwtupload.GWTUpload"/>
    <inherits name="com.google.gwt.junit.JUnit"/>

    <!-- Specify the app entry point class. -->
    <entry-point class='fileuploadfacility.client.FileUploadFacility'/>

    <!-- Specify the app servlets. -->

    <servlet path='/fileuploadfacility.FileUploadFacility/FileUploadFacilityService'
             class='fileuploadfacility.server.FileUploadFacilityServiceImpl'/>


    <servlet path='/servlet.gupld' class='fileuploadfacility.server.FileUploadFacilityServlet'/>


</module>
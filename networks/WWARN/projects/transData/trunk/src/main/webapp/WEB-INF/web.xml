<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>transData</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>DataController</display-name>
    <servlet-name>DataController</servlet-name>
    <servlet-class>org.cggh.tools.transData.data.DataController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DataController</servlet-name>
    <url-pattern>/data/*</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>FunctionsController</display-name>
    <servlet-name>FunctionsController</servlet-name>
    <servlet-class>org.cggh.tools.transData.functions.FunctionsController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FunctionsController</servlet-name>
    <url-pattern>/functions/*</url-pattern>
  </servlet-mapping>
</web-app>